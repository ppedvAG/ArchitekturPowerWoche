@model IEnumerable<MoviNext.Model.Umrichter>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Spannung)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Leistung)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Frequenz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UmrichterParameter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Version)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NetzAdr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BasisParameter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Spannung) @Html.DisplayFor(modelItem => item.SpannungsEinheit)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Leistung) @Html.DisplayFor(modelItem => item.LeistungsEinheit)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Frequenz) @Html.DisplayFor(modelItem => item.FrequenzEinheit)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UmrichterParameter)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Version)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NetzAdr)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BasisParameter)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id=item.Id  }) |
                    @Html.ActionLink("Details", "Details", new{ id=item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new {  id=item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
